[run]
branch = True
source = avocado

[report]
omit =
    avocado/admin.py
    avocado/forms.py
    avocado/core/decorators.py
    avocado/migrations/*
    avocado/__init__.py
    avocado/*/__init__.py
    avocado/*/*/__init__.py
    avocado/*/*/*/__init__.py

exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    def __unicode__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplemented(Error)?
    if 0:
    if __name__ == .__main__.:

[html]
directory = docs/coverage
